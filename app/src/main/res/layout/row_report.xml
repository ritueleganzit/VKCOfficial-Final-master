<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cardview"
    android:layout_below="@+id/toolbar"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    app:cardBackgroundColor="#ffffff"
    app:cardCornerRadius="5dp"
    app:cardElevation="5dp"
app:contentPadding="5dp"
    app:cardUseCompatPadding="true">



    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:padding="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

     <TextView
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:fontFamily="@font/poppins_medium"
         android:textColor="@color/black"
         android:text="PERFORMANCE REPORT"/>

        <TableLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TableRow>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="SR#"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textColor="@color/gray"></TextView>
                <TextView android:layout_width="wrap_content"
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:text="INPUT QTY" android:layout_weight="1"
                    android:textColor="@color/gray"></TextView>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="OUTPUT QTY"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textColor="@color/gray"></TextView>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="MONTH"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textColor="@color/gray"></TextView>
            </TableRow>
            <TableRow>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textColor="@color/black"></TextView>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="1000" android:layout_weight="1"
                    android:textColor="@color/black"></TextView>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="9000"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textColor="@color/black"></TextView>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Jan-2019"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textColor="@color/black"></TextView>
            </TableRow>
            <TableRow>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textColor="@color/black"></TextView>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1000" android:layout_weight="1"
                    android:gravity="center"
                    android:textColor="@color/black"></TextView>
                <TextView android:layout_width="wrap_content"

                    android:layout_height="wrap_content"
                    android:text="9000"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textColor="@color/black"></TextView>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Feb-2019"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textColor="@color/black"></TextView>
            </TableRow>
            <TableRow>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="3"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textColor="@color/black"></TextView>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="1000" android:layout_weight="1"
                    android:textColor="@color/black"></TextView>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="9000"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textColor="@color/black"></TextView>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Mar-2019"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textColor="@color/black"></TextView>
            </TableRow>
            <TableRow>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="4"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textColor="@color/black"></TextView>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="1000" android:layout_weight="1"
                    android:textColor="@color/black"></TextView>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="9000"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textColor="@color/black"></TextView>
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Apr-2019"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textColor="@color/black"></TextView>
            </TableRow>
        </TableLayout>
        </LinearLayout>

    </FrameLayout>

</android.support.v7.widget.CardView>